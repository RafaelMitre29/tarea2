  perro2.marea('muyjuerte')
  perro2.desinhibe('https://upload.wikimedia.org/wikipedia/commons/thumb/a/a8/Michelada_con_chile.jpg/245px-Michelada_con_chile.jpg')
})(console.log);

/* Clases */
(function (c) {
  /* Una clase es un modelo a seguir, un machote */
  class Chela {
    //El constructor es un método especial que se ejecuta en el momento de instanciar la clase
    constructor (nombre, gradodealcohol, marea, marca, tipo) {
      //Definición de los Atributos de mi clase
      this.nombre = nombre
      this.gradodealcohol = gradodealcohol
      this.marea = marea
      this.marca = marca
      this.tipo = tipo
    }

    //Definición de los métodos de mi clase
    emborracha () {
      c('Salud!!!')
    }

    marea (efecto = 'depende cuantas tomes') {
      c(`${this.nombre} cuantas ${unasdiez}`)
    }

    desinhibe (imagen) {
      document.write(`<img src="${imagen}">`)
    }
  }

  //La palabra new me permite crear o INSTANCIAR un objeto de un tipo determinado de clase
  const caguama = new Chela('caguama', 3, false, 'victoria', 'oscura')
  const tonayan = new Perro('tonayan', 7, true, 'tonayan', 'de caña')

  c(
    caguama,
    caguama.nombre,
    caguama.gradodealcohol,
    caguama.marea,
    caguama.marca,
    caguama.tipo,
    tonayan,
    tonayan.nombre,
    tonayan.gradodealcohol,
    tonayan.marea,
    tonayan.marca,
    tonayan.tipo,
  )

  caguama.emborracha()
  caguama.marea()
 caguama.desinhibe('https://www.google.com.mx/url?sa=i&rct=j&q=&esrc=s&source=images&cd=&ved=0ahUKEwie29-tyP_WAhXphlQKHe_pAc8QjBwIBA&url=http%3A%2F%2Fwww.notimundo.com.mx%2Fwp-content%2Fuploads%2F2017%2F02%2Fcaguamas-sudada1.jpg&psig=AOvVaw3bMu-EYnoPyn_GXbL2oSoy&ust=1508601399581301')

  tonayan.emborracha()
  tonayan.marea()
  tonayan.desinhibe('https://assets.metrolatam.com/mx/2017/03/03/1506593_764162460279329_430394972_n-1200x800.jpg')
})(console.log);
