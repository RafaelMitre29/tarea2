// Comentario de una línea


  
   const alcohol = {
    nombre: 'chela',
    gradodealcohol: 5,
    marea: true,
    marca: 'indio',
    tipo: 'oscura',
    emborracha: function () {
      c('bien pedo!!!')
    },
    marea: function (efecto = 'tecaes') {
      c(`${this.nombre} donde ${alsuelo}`)
    },
    desinhibe: function (imagen) {
      document.write(`<img src="${imagen}">`)
    }
  }

  c(
    chela,
   chela.nombre,
    chela.gradodealcohol,
    chela.marea,
    chela.marca,
    chela.tipo
  )

  perro.emborracha()
  perro.marea()
  perro.marea('tecaes')
  perro.desinhibe('https://i2.wp.com/www.liquorlab.com.mx/wp-content/uploads/2015/10/indio-botella.jpeg?fit=460%2C440')

  const chela2 = {
    nombre: 'michelada',
    gradodealcohol: 6,
    marea: true,
    marca: 'sol',
    tipo: 'clara',
    emborracha: function () {
      c('después de varias!!!')
    },
    marea: function (efecto = 'muyjuerte') {
      c(`${this.nombre} donde ${camavoladora}`)
    },
    desinhibe: function (imagen) {
      document.write(`<img src="${imagen}">`)
    }
  }

  c(
    chela2,
    chela2.nombre,
    chela2.gradodealcohol,
    chela2.marea,
    chela2.marca,
    chela2.tipo
  )

  perro2.emborracha()
